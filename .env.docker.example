# Docker Compose Environment Variables
# Copy this file to .env and fill in the values before running docker-compose

# MongoDB
MONGO_USER=admin
MONGO_PASSWORD=CHANGE_ME_TO_A_STRONG_PASSWORD
MONGO_DB=comspace

# Redis
REDIS_PASSWORD=CHANGE_ME_TO_A_STRONG_PASSWORD

# Backend
NODE_ENV=production
JWT_SECRET=CHANGE_ME_TO_A_RANDOM_64_CHAR_STRING
JWT_REFRESH_SECRET=CHANGE_ME_TO_A_DIFFERENT_RANDOM_64_CHAR_STRING
FRONTEND_URL=https://your-domain.com

# Frontend
NEXT_PUBLIC_API_URL=https://api.your-domain.com
